{
    "name": "sntools/types",
    "description": "Strong type handling.",
    "type": "library",
    "keywords": ["type", "class", "autoboxing"],
    "authors": [
        {
            "name": "Samy NAAMANI",
            "email": "samy@namani.net"
        }
    ],
    "require": {
        "PHP": ">=5.3",
        "sntools/core": "*"
    },
    "suggest": {
        "ext-operator": "*"
    },
    "autoload": {
        "psr-4": {
            "SNTools\\Types\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpunit/phpunit-skeleton-generator": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "SNTools\\Types\\Tests\\": "tests/"
        }
    }
}
